<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Assets, less, js -->
    <template id="assets_frontend" inherit_id="website.assets_frontend">
        <xpath expr="." position="inside">
            <link href="/website_shop_product_brands/static/src/css/style.css" rel='stylesheet' type='text/css'/>
        </xpath>
    </template>

    <template id="assets_editor" inherit_id="website.assets_editor">
        <xpath expr="." position="inside">
            <script src="/website_shop_product_brands/static/src/js/website_shop_product_brands.js" type="text/javascript"></script>
        </xpath>
    </template>
</odoo>
